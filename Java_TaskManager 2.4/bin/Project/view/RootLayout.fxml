<BorderPane prefHeight="480.0" prefWidth="830" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Project.view.RootController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Logout">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onMenuClose" text="Close" />
